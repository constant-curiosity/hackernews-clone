import gql from "graphql-tag";

const NEW_LINKS_SUBSCRIPTION = gql`
  subscription {
    newLink {
      id
      url
      description
      createdAt
      postedBy {
        id
        name
      }
      votes {
        id
        user {
          id
        }
      }
    }
  }
`;

export default NEW_LINKS_SUBSCRIPTION;
//Location: LinkList.jsx
